{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-native",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}
